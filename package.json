{
  "name": "atlas-of-housing-production",
  "version": "0.0.1",
  "description": "A boilerplate for building a fast static website with the Eleventy static site generator, for deployment to Netlify.",
  "scripts": {
    "start": "eleventy --serve & postcss styles/tailwind.css --o _tmp/style.css --watch",
    "build": "ELEVENTY_PRODUCTION=true eleventy & NODE_ENV=production postcss styles/tailwind.css --o _site/style.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rodrigoescandon/atlas-of-housing-production"
  },
  "author": {
    "name": "Rodrigo Escand√≥n Cesarman",
    "email": "rodrigo.escandon.cesarman@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rodrigoescandon/atlas-of-housing-production/issues"
  },
  "homepage": "https://github.com/rodrigoescandon/atlas-of-housing-production",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "html-minifier": "^4.0.0",
    "luxon": "^1.22.2",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.2.7",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.6.2"
  },
  "dependencies": {
    "@11ty/eleventy-navigation": "^0.1.5",
    "clean-css": "^4.2.1",
    "uglify-es": "^3.3.9"
  }
}
